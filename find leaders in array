def find_leaders(arr):
    n = len(arr)
    if n == 0:
        return []
    leaders = []
    max_from_right = arr[n-1]
    leaders.append(max_from_right)
    for i in range(n-2, -1, -1):
        if arr[i] > max_from_right:
            max_from_right = arr[i]
            leaders.append(max_from_right)
    return leaders[::-1]
nums = [16, 17, 4, 3, 5, 2]
print(find_leaders(nums)) # Output: [17, 5, 2]
